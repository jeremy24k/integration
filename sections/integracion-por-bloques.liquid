<section class="slider-moxiepro">

    <div class="container-slider-moxiepro">

        {% if section.settings.title != blank %}
            <h2 class="title-slider-moxiepro">
                {{ section.settings.title }}
            </h2>
        {% endif %}

        {% if section.settings.sub_title != blank %}
            <h3 class="sub-title-slider-moxiepro">
                {{ section.settings.sub_title }}
            </h3>
        {% endif %}

        <div class="swiper swiper-moxiepro">
            <div class="background-buttom-slider"></div>
            <div class="background-buttom-slider bg-left"></div>
            <div class="image-slider-moxiepro">
                <img src="https://cdn.shopify.com/s/files/1/0317/5694/3419/files/727A9788_1.png?v=1718920220" alt="">
            </div>
            <div class="swiper-wrapper container-slider-item-moxiepro">

                {% if section.blocks.size > 0 %}
                    {% for block in section.blocks %}
                        {% if block.type == 'card-slider-moxiepro' %}

                            <div class="swiper-slide item-slider-moxiepro">
                                <div class="benefit-title">
                                    {% if block.settings.card_header != blank %}
                                        <p>
                                            {{ block.settings.card_header}}
                                        </p>
                                    {% endif %}
                                </div>

                                {% if block.settings.card_title != blank or block.settings.card_description != blank %}
                                    <div class="text-slider-moxiepro">
                                        {% if block.settings.card_title != blank %}
                                            <h3 class="title-text-moxiepro">
                                                {{ block.settings.card_title }}         
                                            </h3>
                                        {% endif %}

                                        {% if block.settings.card_description != blank %}
                                            <p class="text-moxiepro">
                                                {{ block.settings.card_description }}
                                            </p>
                                        {% endif %}
                                    </div>
                                {% endif %}
                             </div>

                        {% endif %}
                    {% endfor %}
                {% endif %}
               
            </div>

